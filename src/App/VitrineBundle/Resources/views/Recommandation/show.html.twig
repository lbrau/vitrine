{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Recommandation</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ entity.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ entity.prenom }}</td>
            </tr>
            <tr>
                <th>Fonction</th>
                <td>{{ entity.fonction }}</td>
            </tr>
            <tr>
                <th>Specialite</th>
                <td>{{ entity.specialite }}</td>
            </tr>
            <tr>
                <th>Typerelation</th>
                <td>{{ entity.typeRelation }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('recommandation') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('recommandation_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
